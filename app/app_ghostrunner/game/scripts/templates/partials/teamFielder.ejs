<td class="with-name"><%= displayText %></td>
<td class="with-select">
	<div class="player-role" data-role="<%= role.enumText %>">
		<span><%= role.displayText %></span>
	</div>
</td>
<td class="with-cost"><%= cost %></td>
<td class="with-select">
	<select name="positions">
		<% _.each(positions, function(position){ %>
			<option value="<%= position.enumText %>"><%= position.displayText %></option>
		<% }) %>
	</select>
</td>
<td class="with-checkbox">
	<input type="checkbox" name="select-player"></input>
</td>